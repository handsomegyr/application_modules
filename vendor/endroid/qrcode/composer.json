{
    "name": "endroid/qrcode",
    "description": "Endroid QR Code",
    "keywords": ["endroid", "qrcode", "qr", "code"],
    "homepage": "https://github.com/endroid/QrCode",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.0",
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\QrCode\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
